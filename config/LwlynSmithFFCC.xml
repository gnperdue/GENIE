<?xml version="1.0" encoding="ISO-8859-1"?>

<alg_conf>

<!--
Configuration sets for Llewellyn-Smith QEL CC form factors

Configurable Parameters:
..........................................................................................................
Name                     Type     Optional   Comment                     Default
..........................................................................................................
QEl-Ma                       double   Yes        Axial  Mass squared         GPL: QEl-Ma
QEl-FA0                      double   Yes        FA(q2=0)                    GPL: QEL-FA0
ElasticFormFactorsModel  alg      No         Elastic form factors model  GPL: ElasticFormFactorsModel
AxialFormFactorModel     alg      No         Axial form factor model     GPL: AxialFormFactorModel
WeinbergAngle            double   Yes        Weinberg angle              GPL: WeinbergAngle

-->

  <param_set name="Default"> 
     <param type="double" name="WeinbergAngle"> 0.49744211 </param>

     <param type="double" name="QEL-FA0"> -1.2670 </param>

  </param_set>

  <param_set name="Dipole"> 
     <param type="alg" name="ElasticFormFactorsModel"> genie::DipoleELFormFactorsModel/Default </param>
     <param type="alg" name="AxialFormFactorModel"> genie::DipoleAxialFormFactorModel/Default </param>

     <param type="double" name="WeinbergAngle"> 0.49744211 </param>
     <param type="double" name="QEL-FA0"> -1.2670 </param>
  </param_set>

  <param_set name="ZExp"> 
     <param type="alg" name="AxialFormFactorModel"> genie::ZExpAxialFormFactorModel/Default </param>

     <param type="double" name="WeinbergAngle"> 0.49744211 </param>
     <param type="double" name="QEL-FA0"> -1.2670 </param>
  </param_set>

  <param_set name="RunningMA"> 
     <param type="alg" name="AxialFormFactorModel"> genie::KuzminNaumov2016AxialFormFactorModel/Default </param>

     <param type="double" name="WeinbergAngle"> 0.49744211 </param>
     <param type="double" name="QEL-FA0"> -1.2670 </param>
  </param_set>
</alg_conf>

